{
  "name": "rzt-react-boilerplate",
  "version": "1.0.0",
  "description": "react boilerplate with router, redux, sagas, immutable and others",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harish-rzt/react-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/harish-rzt/react-boilerplate#readme",
  "bugs": {
    "url": "https://github.com/harish-rzt/react-boilerplate/issues"
  },
  "scripts": {
    "start": "concurrently --kill-others \"better-npm-run start-dev\"",
    "start-local": "concurrently --kill-others \"better-npm-run start-local\"",
    "start-qa": "concurrently --kill-others \"better-npm-run start-qa\"",
    "clean": "rimraf ./__build__",
    "stylelint": "stylelint ./src/**/*.css --fix",
    "eslint": "eslint **/src/**/*.js --fix",
    "lint": "npm run eslint | npm run stylelint",
    "cleanup": "rm -f package-lock.json && npm i",
    "postdeploy": "sh ./scripts/post_deploy.sh",
    "revert_build": "sh ./scripts/revert_to_previous_build.sh"
  },
  "betterScripts": {
    "start-local": {
      "description": "This script will wun the dev server with default ports",
      "command": "node --max-old-space-size=8192 ./server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 8081,
        "API_HOST": "localhost",
        "API_PORT": 9999,
        "MODULE_NAME": "Local Enviromenment"
      }
    },
    "start-dev": {
      "description": "This script will run the dev server with default ports",
      "command": "node --max-old-space-size=8192 ./server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 8081,
        "API_HOST": "192.168.60.68",
        "API_PORT": 9090,
        "MODULE_NAME": "Default Dev Server"
      }
    }
  },
